//find the sum of all the multiples of 3 and 5 that are less than 1000

public class ProjectEulerProblem1
{

   public static void main(String[] args)
   {
      int sum=0;
      for (int i=0; i<1000; i++)
      {
         if (i%3==0 || i%5==0)
         {
            sum+=i;
         }
      }  
      System.out.println("the answer is "+sum);
    }  
}


