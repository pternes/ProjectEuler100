//find difference between sum of first 100 squares and 
//square of sum of first 100 natural numbers

public class ProjectEulerProblem6
{
   public static void main(String[] args)
   {
      int sumSquares=0;
      int squareSum=0;
      int sum=0;
      for(int i=1; i<=100; i++)
      {
         sumSquares+=(int)Math.pow(i,2);
         sum+=i;
      }   
      squareSum=(int)Math.pow(sum,2);
      System.out.println(squareSum-sumSquares);   
   
   }


}
